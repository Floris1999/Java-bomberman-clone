<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="background" spacing="10" alignment="CENTER" prefWidth="800" prefHeight="800" fx:controller="controller.ScoreController" xmlns:fx="http://javafx.com/fxml">

    <padding><Insets top="0" right="0" bottom="0" left="0"/></padding>

    <fx:define>
        <Font fx:id="FONT" name="Arial" size="20" />
    </fx:define>

    <VBox spacing="10" alignment="CENTER">
        <padding><Insets top="0" right="0" bottom="0" left="0"/></padding>
        <Label fx:id="highscore1" text="Label" prefWidth="400" prefHeight="35" alignment="CENTER"  font="$FONT"/>
        <Label fx:id="highscore2" text="Label" prefWidth="400" prefHeight="35" alignment="CENTER"  font="$FONT"/>
        <Label fx:id="highscore3" text="Label" prefWidth="400" prefHeight="35" alignment="CENTER"  font="$FONT"/>
        <Label fx:id="highscore4" text="Label" prefWidth="400" prefHeight="35" alignment="CENTER"  font="$FONT"/>
        <Label fx:id="highscore5" text="Label" prefWidth="400" prefHeight="35" alignment="CENTER"  font="$FONT"/>
        <padding><Insets top="0" right="0" bottom="0" left="0"/></padding>
    </VBox>

    <VBox spacing="10" alignment="CENTER">
        <Button id="previous" text="Return" prefWidth="200" font="$FONT" onAction="#previous" />
        <padding><Insets top="20" right="25" bottom="20" left="25"/></padding>
    </VBox>
</VBox>
