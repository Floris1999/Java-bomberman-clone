<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox fx:id="background" spacing="10" alignment="CENTER" prefWidth="800" prefHeight="800" fx:controller="controller.LevelController" xmlns:fx="http://javafx.com/fxml">

    <padding><Insets top="00" right="25" bottom="10" left="25"/></padding>

    <fx:define>
        <Font fx:id="FONT" size="18" />
    </fx:define>

    <VBox spacing="10" alignment="CENTER">
        <padding><Insets top="00" right="25" bottom="20" left="0"/></padding>
        <Button id="Button1" text="Level 1" prefWidth="200" font="$FONT" onAction="#Button1" />
        <Button id="Button2" text="Level 2" prefWidth="200" font="$FONT" onAction="#Button2" />
        <Button id="Button3" text="Level 3" prefWidth="200" font="$FONT" onAction="#Button3" />
        <Button id="Button4" text="Level 4" prefWidth="200" font="$FONT" onAction="#Button4" />
        <Button id="Button5" text="Level 5" prefWidth="200" font="$FONT" onAction="#Button5" />
        <padding><Insets top="00" right="25" bottom="30" left="25"/></padding>
    </VBox>

    <VBox spacing="10" alignment="CENTER">
        <Button id="previous" text="Return" prefWidth="200" font="$FONT" onAction="#previous" />
        <padding><Insets top="20" right="25" bottom="20" left="25"/></padding>
    </VBox>
</VBox>
