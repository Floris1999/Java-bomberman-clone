<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox fx:id="background" spacing="10" alignment="CENTER" prefWidth="800" prefHeight="800"
      fx:controller="controller.MenuController" xmlns:fx="http://javafx.com/fxml">

    <padding>
        <Insets top="410" right="25" bottom="10" left="25"/>
    </padding>

    <fx:define>
        <Font fx:id="FONT" size="18" />
    </fx:define>

    <HBox spacing="10" alignment="CENTER">
        <Button text="Start" prefWidth="100" font="$FONT" onAction="#buttonStart"  />
        <Button text="Score" prefWidth="100" font="$FONT" onAction="#buttonScore" />
        <Button text="Eindig" prefWidth="100" font="$FONT" onAction="#buttonEindig" />
    </HBox>
</VBox>
